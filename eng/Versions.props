<Project>

    <!-- Set the default version -->
    <Import Project="MainVersion.props" />
    
    <PropertyGroup>
        <MetalamaSamplesVersion>$(MainVersion)$(PackageVersionSuffix)</MetalamaSamplesVersion>
        <MetalamaSamplesAssemblyVersion>$(MainVersion)</MetalamaSamplesAssemblyVersion>
    </PropertyGroup>

    
    <!-- Versions of dependencies -->
    <PropertyGroup>
        <MetalamaVersion>0.4.3-preview</MetalamaVersion>
    </PropertyGroup>

    <!-- Import overrides for the local build -->
    <Import Project="../artifacts/private/MetalamaSamplesVersion.props" Condition="Exists('../artifacts/private/MetalamaSamplesVersion.props')" />
    <Import Project="Dependencies.props" Condition="Exists('Dependencies.props')"/>

    

    <PropertyGroup>
        <AssemblyVersion>$(MetalamaSamplesAssemblyVersion)</AssemblyVersion>
        <Version>$(MetalamaSamplesVersion)</Version>
    </PropertyGroup>

</Project>