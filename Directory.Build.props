<Project>
    <Import Project="..\Caravela\artifacts\private\CaravelaVersion.props" Condition="Exists('Caravela.local')"/>
    <PropertyGroup>
        <CaravelaVersion Condition="'$(CaravelaVersion)'==''">0.4.1-preview</CaravelaVersion>

        <RestoreAdditionalProjectSources Condition="'$(IS_POSTSHARP_OWNED)'=='True'">
            $(RestoreAdditionalProjectSources);
            https://nuget.postsharp.net/nuget/caravela/
        </RestoreAdditionalProjectSources>

        <DebugCaravelaAnalyzer>False</DebugCaravelaAnalyzer>
        <DebugCaravelaIde>False</DebugCaravelaIde>
        <CaravelaDebugTransformedCode>True</CaravelaDebugTransformedCode>
        <Nullable>enable</Nullable>

    </PropertyGroup>

</Project>